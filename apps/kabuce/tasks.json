{
  "tasks": {
    "build": [
      "react-scripts build"
    ],
    "deploy": [
      "gh-pages -d build"
    ],
    "eject": [
      "react-scripts eject"
    ],
    "start": [
      "react-scripts start"
    ],
    "test": [
      "cross-env CI=true react-scripts test --all --passWithNoTests"
    ],
    "upgrade": [
      "npm-check -y"
    ]
  }
}
