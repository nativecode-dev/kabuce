{
  "author": "opensource@nativecode.com",
  "dependencies": {
    "cross-fetch": "^3.0.5",
    "node-crud-kit": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "~11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/svelte": "^6.0.21",
    "@tsconfig/svelte": "^1.0.10",
    "@types/mocha": "^8.0.3",
    "@types/node": "^13.13.16",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "cypress": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "fontello-cli": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.2.1",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "rollup": "^2.26.10",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.5",
    "sass-loader": "^9.0.3",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.24.1",
    "svelte-check": "^0.1.58",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  },
  "description": "Svelte Express Node Template",
  "homepage": "https://github.com/Zimtir/SENT-template",
  "keywords": [
    "actions",
    "commitlint",
    "cypress",
    "docker",
    "docker-compose",
    "eslint",
    "express",
    "fontello",
    "husky",
    "node",
    "sapper",
    "scss",
    "stylelint",
    "svelte",
    "prettier",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.(js|ts|json)": [
      "npm run format-and-fix",
      "git add"
    ],
    "src/**/*.(svelte)": [
      "npm run svelte:check"
    ],
    "src/**/*.scss": [
      "npm run stylelint:scss:fix",
      "git add"
    ],
    "src/**/*.css": [
      "npm run stylelint:css:fix",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "name": "@nativecode/kabuce-appui",
  "scripts": {
    "build": "sapper build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "depcheck": "npx depcheck",
    "dev": "sapper dev",
    "docker": "docker build -t nativecode/kabuce-appui .",
    "export": "sapper export --legacy",
    "eslint": "eslint --cache --ext .js,.ts, src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/**/*.ts --max-warnings=0",
    "fix": "npm run prettier && npm run eslint:fix && npm run stylelint:fix && npm run svelte:check",
    "format-and-fix": "npm run prettier && npm run eslint --fix",
    "fontello": "fontello-cli install --config ./fontello.config.json --css ./public/assets/css --font ./public/assets/font",
    "prettier": "prettier --write ./src/**",
    "prettier:root": "npm run prettier:root:json && npm run prettier:root:js",
    "prettier:root:js": "prettier --write ./*.js",
    "prettier:root:json": "prettier --write ./*.json",
    "start": "node __sapper__/build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "stylelint": "npm run stylelint:css && npm run stylelint:scss",
    "stylelint:fix": "npm run stylelint:css:fix && npm run stylelint:scss:fix",
    "stylelint:css": "stylelint src/**/*.css --syntax css --allow-empty-input",
    "stylelint:scss": "stylelint src/**/*.scss --syntax scss --allow-empty-input",
    "stylelint:css:fix": "npm run stylelint:css --fix",
    "stylelint:scss:fix": "npm run stylelint:scss --fix",
    "svelte:check": "svelte-check",
    "test:headless:dev": "start-server-and-test dev http://localhost:3000 test:headless",
    "test:headless:prod": "start-server-and-test start http://localhost:3000 test:headless",
    "test:headless": "cypress run",
    "test:ui": "cypress open",
    "test:ui:dev": "start-server-and-test dev http://localhost:3000 test:ui",
    "test:ui:prod": "start-server-and-test start http://localhost:3000 test:ui",
    "update:install": "npm update && npm install",
    "update:pull": "npm outdated",
    "upgrade": "npm run update:install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zimtir/SENT-template.git"
  },
  "version": "0.3.3"
}
