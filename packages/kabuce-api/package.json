{
  "name": "@kabuce/api",
  "version": "0.0.0",
  "description": "Kabuce API",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc && npm run swagger",
    "lerna:build": "docker build --rm -t nativecode/kabuce-api .",
    "test": "npm run clean && npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "test:headless": "npm run test:coverage",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "npm run build && docker-compose build",
    "deploy": "exit 0",
    "swagger": "ts-node src/index.ts -- generate:swagger",
    "upgrade": "npm update && npm install"
  },
  "author": "opensource@nativecode.com",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "5.65.1",
    "@tsed/common": "5.65.1",
    "@tsed/core": "5.65.1",
    "@tsed/di": "5.65.1",
    "@tsed/exceptions": "5.65.1",
    "@tsed/platform-express": "5.65.1",
    "@tsed/swagger": "5.65.1",
    "ajv": "^6.12.4",
    "atob": "^2.1.2",
    "aws-sdk": "^2.751.0",
    "bcrypt": "^5.0.0",
    "body-parser": "1.19.0",
    "btoa": "^1.2.1",
    "compression": "1.7.4",
    "concurrently": "5.2.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "deepmerge": "^4.2.2",
    "express": "4.17.1",
    "method-override": "^3.0.0",
    "mustache": "^4.0.1",
    "node-fetch": "^2.6.1",
    "node-uuid": "1.4.8",
    "pouchorm": "^1.2.0",
    "sha256": "^0.2.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/atob": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/btoa": "^1.2.3",
    "@types/chai": "4.2.12",
    "@types/chai-as-promised": "7.1.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.6",
    "@types/deepmerge": "^2.2.0",
    "@types/express": "4.17.7",
    "@types/http-proxy": "1.17.2",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "8.0.1",
    "@types/mustache": "^4.0.1",
    "@types/node": "14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.6",
    "@types/request-promise": "4.1.45",
    "@types/sha256": "^0.2.0",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/supertest": "2.0.10",
    "@types/swagger-schema-official": "^2.0.21",
    "@types/uuid": "^8.3.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "mocha": "8.1.1",
    "nodemon": "1.19.4",
    "nyc": "15.1.0",
    "rimraf": "3.0.0",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.8.3"
  }
}
